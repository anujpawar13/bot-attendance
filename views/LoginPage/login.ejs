<link rel="stylesheet" href="/css/LoginPage/login.css" />

<div class="icon">
	<div class="iconimage"></div>
</div>
<div class="wrapper">
	<div class="Sign_In_Form">
		<div>
			<h2>Sign In</h2>
			<br />
			<h4 style="font-weight: lighter">Login to your account.</h4>
			<br />
			<input
				type="text"
				id="login_username"
				placeholder="Username"
				name="username"
				required
			/><br /><br />
			<input type="password" id="login_password" placeholder="Password" name="password" required />
			<br /><br />
			<button id="login">Login</button>
			<br />
			<div style="display: flex">
				<div id="dont_have_a_account">Don't have an account?</div>
				<div id="create_a_new_account">Create a new account</div>
			</div>
		</div>
	</div>
	<div class="image">
		<div class="image_heading"></div>
	</div>
</div>

<script src="/js/LoginPage/login.js"></script>
